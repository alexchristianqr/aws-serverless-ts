{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch: npm run debug",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run-script", "debug"],
      "skipFiles": ["<node_internals>/**"],
      "cwd": "${workspaceFolder}",
      "outFiles": ["${workspaceFolder}/.esbuild/.build/src/**/*.js.map"],
      "sourceMaps": true
    }
  ]
}
