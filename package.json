    "serverless-esbuild": "^1.25.0",
    "@types/aws-lambda": "^8.10.51",
    "serverless-plugin-typescript": "^2.1.5",
    "serverless-offline": "^13.2.0",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  }
}
